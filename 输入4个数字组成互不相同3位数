#include<stdio.h>
void A1(int i, int j, int k)                    //对i,j,k三个数依次排序
{
    printf("%3d ", i * 100 + j * 10 + k);
    printf("%3d ", i * 100 + k * 10 + j);
    printf("%3d ", j * 100 + i * 10 + k);
    printf("%3d ", j * 100 + k * 10 + i);
    printf("%3d ", k * 100 + j * 10 + i);
    printf("%3d \n", k * 100 + i * 10 + j);
}
void A()
{
    int a, b, c, d;
    printf("输入不同的四个数=>");
    scanf("%d%d%d%d", &a, &b, &c, &d);
    A1(a, b, c);
    A1(a, b, d);
    A1(a, c, d);
    A1(b, c, d);
}
